page{
  background: #f2f3f4;
}
.search{
  width: 90%;
  background: #fff;
  margin: 20rpx 0;
  border-radius: 10rpx;
  height: 2em;
}
.pics{
  width: 100%;
}
.pics image{
  width: 100%;
  height: 50vw;
}
.main{
  width: 100%;
  background: #fff;
  margin-top: 20rpx;
}
.title{
  display: flex;
  align-items: center;
  margin: 3vw 0 3vw 3vw;
}
.title .title-w{
  width: 10rpx;
  height: 30rpx;
  background: #f0a232;
  margin-right: 10rpx;
}
.title .title-name{
  flex: 1;
}
.title .title-more{
  width:50rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.title .title-more image{
  width: 17rpx;
  height: 30rpx;
}
.topic{
  display: flex;
}
.topic .topic1 image{
  width: 37vw;
  height: 37vw;
  margin: 0 3vw;
}
.topic .topic2 image{
  width: 54vw;
  height: 17vw;
}
.topic .topic3{
  margin-bottom: 2vw;
}
